{
  "manifest_version": 3,
  "name": "SnipVault",
  "version": "1.0",
  "description": "Save web highlights to your personal markdown note vault",
  "icons": {
    "128": "icons/snipvault.png"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "SnipVault"
  }
}
